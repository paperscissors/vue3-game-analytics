{
  "name": "vue3-game-analytics",
  "version": "0.2.2",
  "description": "Comprehensive analytics tracking system for Vue 3 game applications",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-game-analytics.umd.js",
  "module": "./dist/vue3-game-analytics.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-game-analytics.es.js",
      "require": "./dist/vue3-game-analytics.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "test": "vitest run",
    "test:watch": "vitest",
    "demo": "vite --config demo/vite.config.ts"
  },
  "dependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^26.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "analytics",
    "game",
    "tracking",
    "interaction"
  ],
  "author": "",
  "license": "MIT"
}
